library("vegan")
SH_db <- read.csv("~/Path/to/file.csv", header = TRUE, sep = ",", dec = ",", fill = TRUE)


sample = 10				    #Tamanho da amostra
prefix = "SH_db_r"		#Prefixo do arquivo sub-amostrado
rep = 10			      	#Número de replicatas

for(i in 1:rep) 
{
name <- paste(prefix, i, sep = "")
assign(name, rrarefy(SH_db, sample))

#As replicatas estao prontas, agora falta somar todas os dataframes e fazer uma média de alguma forma

#reduce()?
#


}
